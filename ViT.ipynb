    "learning_rate = 0.001\n",
    "weight_decay = 0.0001\n",
    "batch_size = 256\n",
    "num_epochs = 48\n",
    "image_size = 72 # we'll resize the input images later\n",
    "patch_size = 6 # size of the patches to be extracted from the input image\n",
    "num_patches = (image_size // patch_size) ** 2\n",
    "num_heads = 4\n",
    "projection_dim = 64\n",
    "transformer_units = [\n",
